import java.util.Locale;
import java.io.*;
import java.util.Scanner;

public class Read_and_print{
	public static void main(String[] args){
		String file = "test_file.txt";
		Scanner inputStream = null;
		try{
			inputStream = new Scanner(new FileInputStream(file));
		}	
		catch(FileNotFoundException e){
			System.exit(0);
		}
		inputStream.useLocale(Locale.US);
		long n = inputStream.nextLong();
		String s = inputStream.next();
		String s1 = inputStream.next();
		double d = inputStream.nextDouble();
		System.out.println("Imma write a number and some werds");
		System.out.println(inputStream.nextLong);
		System.out.println();
		System.out.print(s);
		System.out.println();
		System.out.print(s1);
		System.out.println();
		System.out.print(d);
		System.out.println();

	}
}
